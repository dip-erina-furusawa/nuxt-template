{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "prepare": "husky",
    "lint": "npm run lint:es:fix && npm run lint:style:fix",
    "lint:es": "eslint .",
    "lint:es:fix": "eslint . --fix",
    "lint:style": "stylelint './**/*.css'",
    "lint:style:fix": "stylelint --fix './**/*.css'",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,vue,json}'",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nuxt/eslint": "^0.3.10",
    "@nuxtjs/storybook": "^8.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.378.0",
    "nuxt": "^3.11.2",
    "radix-vue": "^1.7.4",
    "shadcn-nuxt": "^0.10.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@nuxtjs/storybook": "7.0.2",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@storybook-vue/nuxt": "0.2.6",
    "@storybook/addon-essentials": "8.0.8",
    "@storybook/addon-interactions": "8.0.8",
    "@storybook/addon-links": "8.0.8",
    "@storybook/blocks": "8.0.8",
    "@storybook/builder-vite": "8.0.8",
    "@storybook/test": "8.0.8",
    "@storybook/vue3": "8.0.8",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^14.10.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "storybook": "8.0.8",
    "stylelint": "^16.5.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.27"
  }
}
